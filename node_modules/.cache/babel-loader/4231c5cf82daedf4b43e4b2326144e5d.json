n      component: Content,\n      meta: {\n        title: '阅读'\n      }\n    }, {\n      name: 'collects',\n      path: '/Collects',\n      component: Collects,\n      meta: {\n        title: '音乐'\n      }\n    },\n],\n  scrollBehavior(to, from, savedPosition) {\n    if (savedPosition) {\n      return savedPosition;\n    } else {\n      return { x: 0, y: 0 }\n    }\n  }\n})\nexport default router\nrouter.beforeEach((to, from, next) => {\n  /* 路由发生变化时修改页面title */\n  if (to.meta.title) {\n    document.title = to.meta.title\n  }\n  next()\n})"]}]}